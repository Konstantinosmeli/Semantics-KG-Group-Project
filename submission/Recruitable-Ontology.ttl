                   rdfs:comment "A type of pizza, topped off with two types of cheese, bacon, ham, pepperoni and hot sausage" .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/NamedPizza
rec:NamedPizza rdf:type owl:Class ;
               rdfs:subClassOf dbr:Pizza .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/PepperoniPizza
rec:PepperoniPizza rdf:type owl:Class ;
                   rdfs:subClassOf rec:NamedPizza ;
                   rdfs:comment "Pepperoni is an American variety of spicy salami made from cured pork and beef seasoned with paprika or other chili pepper" .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/PizzaPlace
rec:PizzaPlace rdf:type owl:Class ;
               rdfs:subClassOf rec:RestaurantType .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/RestaurantType
rec:RestaurantType rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty rec:isTypeOf ;
                                     owl:someValuesFrom dbo:Restaurant
                                   ] .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/StreetName
rec:StreetName rdf:type owl:Class ;
               rdfs:subClassOf rec:AddressLine .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/StreetNumber
rec:StreetNumber rdf:type owl:Class ;
                 rdfs:subClassOf rec:AddressLine .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/VietnamesePizza
rec:VietnamesePizza rdf:type owl:Class ;
                    rdfs:subClassOf rec:NamedPizza ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty rec:hasIngredient ;
                                      owl:allValuesFrom [ rdf:type owl:Class ;
                                                          owl:unionOf ( dbr:Sauce
                                                                        [ owl:intersectionOf ( dbr:Bacon
                                                                                               dbr:Scallion
                                                                                             ) ;
                                                                          rdf:type owl:Class
                                                                        ]
                                                                      )
                                                        ]
                                    ] ;
                    dc:creator "Quang Huy Bui (Bùi Quang Huy)" ;
                    rdfs:label [ rdfs:label "Bánh tráng nướng"@vn
                               ] .


