###  http://dbpedia.org/ontology/name
dbo:name rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty .


###  http://dbpedia.org/ontology/value
dbo:value rdf:type owl:DatatypeProperty .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasStreetName
rec:hasStreetName rdf:type owl:DatatypeProperty .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasStreetNumber
rec:hasStreetNumber rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Ingredients
dbo:Ingredients rdf:type owl:Class .


###  http://dbpedia.org/ontology/Restaurant
dbo:Restaurant rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty rec:hasType ;
                                 owl:someValuesFrom rec:RestaurantType
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty rec:hasAddress ;
                                 owl:allValuesFrom dbo:address
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty dbo:name ;
                                 owl:allValuesFrom xsd:string
                               ] .


###  http://dbpedia.org/ontology/address
dbo:address rdf:type owl:Class ;
            rdfs:label [ rdfs:label "(Synonym) location"@en ,
                                    "địa chỉ"@vn ,
                                    "τοποθεσία"@el
                       ] .


###  http://dbpedia.org/ontology/city
dbo:city rdf:type owl:Class ;
         rdfs:subClassOf dbo:address ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty dbp:locatedIn ;
                           owl:allValuesFrom dbo:state
                         ] .


###  http://dbpedia.org/ontology/country
dbo:country rdf:type owl:Class ;
            rdfs:subClassOf dbo:address .


###  http://dbpedia.org/ontology/postalCode
dbo:postalCode rdf:type owl:Class ;
               rdfs:subClassOf dbo:address ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty dbp:locatedIn ;
                                 owl:allValuesFrom dbo:city
                               ] ;
               rdfs:label [ rdfs:label "(Synonym) zip code"@en
                          ] .


###  http://dbpedia.org/ontology/state
dbo:state rdf:type owl:Class ;
          rdfs:subClassOf dbo:address ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dbp:locatedIn ;
                            owl:allValuesFrom dbo:country
                          ] .


###  http://dbpedia.org/resource/Bacon
dbr:Bacon rdf:type owl:Class ;
          rdfs:subClassOf dbr:Meat ;
          rdfs:label [ rdfs:label "speck"@de ,
                                  "thịt lợn muối xông khói"@vn ,
                                  "tocino"@es
                     ] .


###  http://dbpedia.org/resource/Cheese
dbr:Cheese rdf:type owl:Class ;
           rdfs:subClassOf dbo:Ingredients .


###  http://dbpedia.org/resource/Chili_sauce_and_paste
dbr:Chili_sauce_and_paste rdf:type owl:Class ;
                          rdfs:subClassOf dbr:Sauce .


###  http://dbpedia.org/resource/Currency
dbr:Currency rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty rec:isCurrencyOf ;
                               owl:someValuesFrom dbr:Currency
                             ] .


###  http://dbpedia.org/resource/Feta
dbr:Feta rdf:type owl:Class ;
         rdfs:subClassOf dbr:Cheese .


###  http://dbpedia.org/resource/Fruit
dbr:Fruit rdf:type owl:Class ;
          rdfs:subClassOf dbo:Ingredients ;
          owl:disjointWith dbr:Meat ,
                           dbr:Vegetable .


###  http://dbpedia.org/resource/Mayonnaise
dbr:Mayonnaise rdf:type owl:Class ;
               rdfs:subClassOf dbr:Sauce .


###  http://dbpedia.org/resource/Meat
dbr:Meat rdf:type owl:Class ;
         rdfs:subClassOf dbo:Ingredients ;
         owl:disjointWith dbr:Vegetable .


###  http://dbpedia.org/resource/Menu
dbr:Menu rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty rec:listing ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass <http://dbpedia.org/resource/Dish_(food)>
                         ] .


###  http://dbpedia.org/resource/Mushroom
dbr:Mushroom rdf:type owl:Class ;
             rdfs:subClassOf dbr:Vegetable .


###  http://dbpedia.org/resource/Olive
dbr:Olive rdf:type owl:Class ;
          rdfs:subClassOf dbr:Fruit .


###  http://dbpedia.org/resource/Pepperoni
dbr:Pepperoni rdf:type owl:Class ;
              rdfs:subClassOf dbr:Meat .


###  http://dbpedia.org/resource/Pineapple
dbr:Pineapple rdf:type owl:Class ;
              rdfs:subClassOf dbr:Fruit ;
              rdfs:label [ rdfs:label "La piña"@es ,
                                      "ananas"@de ,
                                      "ananas"@fr
                         ] .


###  http://dbpedia.org/resource/Pizza
dbr:Pizza rdf:type owl:Class ;
          rdfs:subClassOf <http://dbpedia.org/resource/Dish_(food)> ;
          rdfs:comment "An Italian dish consisting of a flat round bread base with cheese, tomatoes, vegetables, meat, etc. on top" .


###  http://dbpedia.org/resource/Pizza_Margherita
dbr:Pizza_Margherita rdf:type owl:Class ;
                     rdfs:subClassOf rec:NamedPizza ;
                     rdfs:comment "Is a typical Neapolitan pizza, made with San Marzano tomatoes, mozzarella cheese, fresh basil, salt, and extra-virgin olive oil" .


###  http://dbpedia.org/resource/Price
dbr:Price rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty rec:hasCurrency ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass dbr:Currency
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dbo:value ;
                            owl:allValuesFrom xsd:float
                          ] .


###  http://dbpedia.org/resource/Sauce
dbr:Sauce rdf:type owl:Class ;
          rdfs:subClassOf dbo:Ingredients .


###  http://dbpedia.org/resource/Scallion
dbr:Scallion rdf:type owl:Class ;
             rdfs:subClassOf dbr:Vegetable ;
             rdfs:label "Spring onion" .


###  http://dbpedia.org/resource/Tomato
dbr:Tomato rdf:type owl:Class ;
           rdfs:subClassOf dbr:Vegetable ;
           rdfs:label [ rdfs:label "cà chua"@vn ,
                                   "ντομάτα"@el
                      ] .


###  http://dbpedia.org/resource/Vegetable
dbr:Vegetable rdf:type owl:Class ;
              rdfs:subClassOf dbo:Ingredients .


###  http://dbpedia.org/resource/Dish_(food)
<http://dbpedia.org/resource/Dish_(food)> rdf:type owl:Class ;
                                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                            owl:onProperty rec:hasPrice ;
                                                            owl:allValuesFrom dbr:Price
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty rec:hasIngredient ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass dbo:Ingredients
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty rec:isListedIn ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass dbr:Menu
                                                          ] .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/AddressLine
rec:AddressLine rdf:type owl:Class ;
                rdfs:subClassOf dbo:address ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dbp:locatedIn ;
                                  owl:allValuesFrom dbo:postalCode
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty rec:hasStreetName ;
                                  owl:allValuesFrom xsd:string
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty rec:hasStreetNumber ;
                                  owl:allValuesFrom xsd:nonNegativeInteger
                                ] .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/AmericanRestaurant
rec:AmericanRestaurant rdf:type owl:Class ;
                       rdfs:subClassOf rec:RestaurantType .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/Bakery
rec:Bakery rdf:type owl:Class ;
           rdfs:subClassOf rec:RestaurantType ;
           rdfs:comment "A place where bread and cakes are made and/or sold" ;
           rdfs:label [ rdfs:label "tiệm bánh"@vn ,
                                   "αρτοποιείο"@el
                      ] .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/Bar
rec:Bar rdf:type owl:Class ;
        rdfs:subClassOf rec:RestaurantType ;
        rdfs:comment "A place where you can buy and drink alcoholic and other drinks" .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/CheesePizza
rec:CheesePizza rdf:type owl:Class ;
                rdfs:subClassOf rec:NamedPizza .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/CoffeeShop
rec:CoffeeShop rdf:type owl:Class ;
               rdfs:subClassOf rec:RestaurantType ;
               rdfs:comment "A small restaurant, often in a store, hotel, etc., where coffee, tea, other drinks without alcohol and simple food are served" .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/GreekRestaurant
rec:GreekRestaurant rdf:type owl:Class ;
                    rdfs:subClassOf rec:RestaurantType ;
                    dc:creator "Konstantinos Meli (Kωνσταντίνος Μελή)" ;
                    rdfs:comment "A Greek restaurant is a restaurant that specializes in Greek cuisine. Their food is colorful and incredibly nutritious. In addition Greek food has a reputation for being heart healthy. This is because it uses olive oil, fish, lean meats, vegetables, herbs and grain." .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/HawaiianPizza
rec:HawaiianPizza rdf:type owl:Class ;
                  rdfs:subClassOf rec:NamedPizza ;
                  rdfs:comment "Hawaiian pizza is a pizza originating in Canada, and is traditionally topped with pineapple, tomato sauce, cheese, and either ham or bacon." .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/KonstantinosPizza
rec:KonstantinosPizza rdf:type owl:Class ;
                      rdfs:subClassOf rec:NamedPizza ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty rec:hasIngredient ;
                                        owl:allValuesFrom [ rdf:type owl:Class ;
                                                            owl:unionOf ( dbr:Feta
                                                                          dbr:Olive
                                                                          dbr:Tomato
                                                                        )
                                                          ]
                                      ] ;
                      dc:creator "Konstantinos Meli (Kωνσταντίνος Μελή)" .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/MeatLoverPizza
rec:MeatLoverPizza rdf:type owl:Class ;
