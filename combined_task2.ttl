            owl:intersectionOf ( cw:Pizza [ a owl:Class ;
                        owl:intersectionOf ( [ a owl:Restriction ;
                                    owl:onProperty cw:hasIngredient ;
                                    owl:someValuesFrom cw:Feta ] [ a owl:Restriction ;
                                    owl:onProperty cw:hasIngredient ;
                                    owl:someValuesFrom cw:Olives ] [ a owl:Restriction ;
                                    owl:onProperty cw:hasIngredient ;
                                    owl:someValuesFrom cw:Spinach ] ) ] ) ] .

