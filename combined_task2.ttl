cw:PizzaMarinara a owl:Class ;
    rdfs:label "Pizza marinara" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cw:hasIngredient ;
            owl:someValuesFrom cw:Garlic ],
        [ a owl:Restriction ;
            owl:onProperty cw:hasIngredient ;
            owl:someValuesFrom cw:Mozzarella ],
