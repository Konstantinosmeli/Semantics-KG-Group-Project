#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/property/locatedIn
dbp:locatedIn rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasAddress
rec:hasAddress rdf:type owl:ObjectProperty ;
               owl:inverseOf rec:isAddressOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:AsymmetricProperty ;
               rdfs:domain dbo:Restaurant ;
               rdfs:range dbo:address .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasCurrency
rec:hasCurrency rdf:type owl:ObjectProperty ;
                owl:inverseOf rec:isCurrencyOf ;
                rdf:type owl:AsymmetricProperty ;
                rdfs:domain dbr:Price ;
                rdfs:range dbr:Currency .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasIngredient
rec:hasIngredient rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf rec:isIngredientOf ;
                  rdf:type owl:AsymmetricProperty ;
                  rdfs:domain <http://dbpedia.org/resource/Dish_(food)> ;
                  rdfs:range dbo:Ingredients .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasPrice
rec:hasPrice rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf rec:isPriceOf ;
             rdf:type owl:AsymmetricProperty ;
             rdfs:domain <http://dbpedia.org/resource/Dish_(food)> ;
             rdfs:range dbr:Price .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/hasType
rec:hasType rdf:type owl:ObjectProperty ;
            owl:inverseOf rec:isTypeOf ;
            rdf:type owl:AsymmetricProperty ;
            rdfs:domain dbo:Restaurant ;
            rdfs:range rec:RestaurantType .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/isAddressOf
rec:isAddressOf rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain dbo:address ;
                rdfs:range dbo:Restaurant .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/isCurrencyOf
rec:isCurrencyOf rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/isIngredientOf
rec:isIngredientOf rdf:type owl:ObjectProperty ,
                            owl:AsymmetricProperty .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/isListedIn
rec:isListedIn rdf:type owl:ObjectProperty ;
               owl:inverseOf rec:listing ;
               rdfs:domain <http://dbpedia.org/resource/Dish_(food)> ;
               rdfs:range dbr:Menu .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/isPriceOf
rec:isPriceOf rdf:type owl:ObjectProperty ,
                       owl:AsymmetricProperty .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/isTypeOf
rec:isTypeOf rdf:type owl:ObjectProperty ,
                      owl:AsymmetricProperty ;
             rdfs:domain rec:RestaurantType ;
             rdfs:range dbo:Restaurant .


###  http:/www.city.ac.uk/inm713-in3067/2023/recruitable/listing
rec:listing rdf:type owl:ObjectProperty ;
            rdfs:domain dbr:Menu ;
            rdfs:range <http://dbpedia.org/resource/Dish_(food)> .


